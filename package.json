{
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "analyze": "polymer analyze > analysis.json",
    "sync": "browser-sync start --config 'config/bs-config.js'",
    "preserve": "yarn sync & yarn analyze",
    "serve": "polymer serve",
    "prepages": "yarn analyze && mkdir pages-dist && cp -r bower_components/* pages-dist/ && mkdir pages-dist/gen-test && cp -r *.html *.js analysis.json demo test pages-dist/gen-test/",
    "pages": "gh-pages -d pages-dist",
    "postpages": "rimraf pages-dist",
    "prerelease": "yarn pages",
    "release": "release-it --config='config/.release-it.json'"
  },
  "dependencies": {
    "browser-sync": "^2.18.13",
    "gh-pages": "^1.0.0",
    "release-it": "^3.2.0"
  },
  "version": "0.0.13"
}
